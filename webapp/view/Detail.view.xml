<mvc:View
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
	controllerName="com.sap.build.standard.untitledPrototype.controller.Detail">
	<Page title="{i18n>TitleDetail}">
		<content>
			<VBox class="sapUiSmallMargin">
				<f:Form id="FormDisplay471"
			editable="false">
					<f:title>
						<core:Title text="{i18n>Items}" />
					</f:title>
					<f:layout>
						<f:ResponsiveGridLayout
					labelSpanXL="4"
					labelSpanL="4"
					labelSpanM="4"
					labelSpanS="12"
					adjustLabelSpan="false"
					emptySpanXL="1"
					emptySpanL="1"
					emptySpanM="1"
					emptySpanS="0"
					columnsXL="1"
					columnsL="1"
					columnsM="1"
					singleContainerFullSize="false" />
					</f:layout>
					<f:formContainers>
						<f:FormContainer>
							<f:formElements>
								<f:FormElement label="{i18n>ID}">
									<f:fields>
										<Text text="{ID}" id="ID"  />
									</f:fields>
								</f:FormElement>

								<f:FormElement label="{i18n>SUM}">
									<f:fields>
										<Input  value="{SUM}" id="SUM" />
									</f:fields>
								</f:FormElement>
								<f:FormElement label="{i18n>description}">
									<f:fields>
										<Input  value="{description}" id="description" />
									</f:fields>
								</f:FormElement>
							</f:formElements>
						</f:FormContainer>
					</f:formContainers>
				</f:Form>
			</VBox>
		</content>
		<footer>
			<Toolbar>
				<Button text="{i18n>Save}" press="_onSavePress" />
				<Button text="{i18n>Delete}" press="_onDeletePress" />
			</Toolbar>
		</footer>
	</Page>
</mvc:View>